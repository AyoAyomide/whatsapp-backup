{
    "name": "Whatsapp Exporter",
    "description": "Contact exporter to csv",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": "icon/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/idb.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://web.whatsapp.com/*"
    ]
}