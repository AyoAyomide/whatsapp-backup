{
    "name": "Whatsapp Exporter",
    "description": "Contact exporter to csv",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon/icon-16.png",
        "32": "icon/icon-32.png",
        "48": "icon/icon-48.png",
        "128": "icon/icon-128.png"
    },
    "action": {
        "default_icon": {
            "16": "icon/icon-16.png",
            "32": "icon/icon-32.png",
            "48": "icon/icon-48.png",
            "128": "icon/icon-128.png"
        },
        "default_title": "Refresh contact list",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/*"
            ],
            "js": [
                "scripts/lib/idb.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://web.whatsapp.com/*"
    ]
}